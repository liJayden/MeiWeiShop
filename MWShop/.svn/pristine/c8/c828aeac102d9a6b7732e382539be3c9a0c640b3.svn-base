<body>
	<jsp:include page="/common/head.jsp"></jsp:include>
	
	<!--导航结束-->
	<div class="clear"></div>

	<!--位置-->
	<div class="user_here center">所在的位置：首页 > 商品详情</div>
	<div class="ibody" style="margin-top:10px">
		 <!--参数-->
		<div class="extraBox">
			<!--产品参数开始-->
			<div>
				<div id="preview" class="spec-preview">
					<c:forEach items="${Productlist}" var="Productlist">
					<div class="jqzoom"><img jqimg="${pageContext.request.contextPath}/img/other/1.jpg" src="${pageContext.request.contextPath}/${Productlist.image}" width="350" height="350" /></div>
					<!--缩图结束-->
					<div class="scAndfx">
						<a href="${pageContext.request.contextPath}/jsp/shoucang.jsp" class="sc-btn fl">加入收藏</a> 
						<script>
							window._bd_share_config={
										"common":{
										"bdSnsKey":{},"bdText":"","bdMini":"2","bdPic":"","bdStyle":"0","bdSize":"16"},"share":{}
												};
							with(document)0[(getElementsByTagName('head')[0]||body)
							.appendChild(createElement('script'))
							.src='http://bdimg.share.baidu.com/static/api/${pageContext.request.contextPath}/js/share.js?v=89860593.js?cdnversion='+~(-new Date()/36e5)];
						</script>
					</div>
					
				</div>

				<div class="extra-text">
					<div class="extra-text-header">
						<h1 style="font-size: 28;color: black;">${Productlist.pname }</h1>
						<br>
						<h1>${Productlist.pdesc }</h1>
					</div>
					<div class="extra-text-body">
						<table width="100%" border="0" cellspacing="0" cellpadding="0">
						  <tr>
							<td width="10" >现价：</td>
							<td style="position:relative;right: 150px "><span class="size20 color4">￥${Productlist.shopprice }</span>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;原价：<span style="text-decoration:line-through;">￥${Productlist.marketprice}</span></td>
						  </tr>
						  <tr>
					<%-- 指定要调用的方法 --%>
							<form id="form" action="<c:url value='/CartController/addCart'/>" method="post">
							<input type="hidden" name="method" value="addCart" />
							<input type="hidden" name="pid" value="${Productlist.pid }" />
							<td >选择数量：</td>
							<td style="position:relative;right: 110px ">
							<input type="text" size="3" name="count" value="1"  style="color: black" />					  
							</td>
							</form>	
						  </tr>
						</table>
					</div>
					<div class="buyBottonBox">
						<a href="javascript:document.getElementById('form').submit();" class="buyBotton-one fl">加入购物车</a>
						
					</div>
					<div>
						注意：此商品可提供普通发票，可提供增值税发票。
					</div>
				</div>
				
			</div>

		</div>

		<div class="clear"></div>
		<!--评价/店铺-->
		<div class="evaluationBox center">
			<div class="evaluation-left fl">
				<div class="e-l-menu">
					<ul>
						<li><a href="#" class="menub6_1" id="menu_6_1" onmouseover=showtabs6(6,1,5);>商品介绍</a></li>
					</ul>
				</div>
				<div class="e-l-text"  id="menutab_6_1">
					<div style="padding:10px">
						<table width="100%" border="0" cellspacing="0" cellpadding="0">
						  <tr height="40px"  >
							<td width="90" >商品名称：</td>
							<td width="300">${Productlist.pname }</td>
							<td width="100" >上架时间：</td>
							<td width="200"> <f:formatDate pattern="yyyy-MM-dd HH:mm:ss" value="${Productlist.pdate }" /></td>
						  </tr>
						  <tr height="40px" >
							<td width="100" >商品价格：</td>
							<td width="200">${Productlist.shopprice }</td>
							<td width="100" >市场价格：</td>
							<td width="200">${Productlist.marketprice }</td>
						  </tr>
						  <tr height="40px" >
							<td width="100">商品特点：</td>
							<td>${Productlist.pdesc }</td>
						  </tr>
						</table>
					</div>
					<div style="line-height:30px; border-top:1px solid #dedede ; padding-top:10px">
					<h1 style="position: relative; left: 30px">图片详情</h1>
					<img align="middle" src="${pageContext.request.contextPath}/${Productlist.image}" width="500" height="500" />
					</div>
					
				</div>
			
			</div>
			<div class="evaluation-right fr">
				
				<div class="all-dp-box" style=" margin-top:0px;">
					<div class="all-dp-head">产品搜索</div>
					<div class="all-dp-body center">
						<table width="100%" border="0" cellspacing="0" cellpadding="0">
						  <tr>
							<td>关键字：</td>
							<td><input type="text" name="textfield" class="dp-text gjz-text" /></td>
						  </tr>
						  <tr>
							<td>价格：</td>
							<td>
							<input type="text" name="textfield" class="dp-text jg-text"  /> - <input type="text" name="textfield" class="dp-text jg-text"  />
							</td>
						  </tr>
						  <tr>
							<td>&nbsp;</td>
							<td><input type="submit" name="Submit" value="提交" class="dp-Submit" /></td>
						  </tr>
						</table>

					</div>
				</div>
				
			</div>
		</div>
		</c:forEach>
		<!--热卖商品-->
		<div class="remai_goods center">
			<div class="remai_goods_title">热卖商品</div>
			<ul>
			<c:forEach items="${isHotProduct}" var="isHotProduct" >
				<li>
					<p><img src="${pageContext.request.contextPath}/${isHotProduct.image}" width="160" height="160" /></p>
					<p><span class="color4 size14-b">￥${isHotProduct.shopprice}</span>&nbsp;&nbsp;<del>￥${isHotProduct.marketprice}</del></p>
								<p class="remai_goods_text"><a href="/MWShop/ProductController/findProductByPid?pid=${ isHotProduct.pid}">${isHotProduct.pname}</a></p>
				</li>
			</c:forEach>
			</ul>
		</div>
		
		<div class="clear" style="height:30px"></div>
		
	</div>

