<script type="text/javascript" src="${pageContext.request.contextPath}/js/jquery-1.11.1.js"></script>
<script type="text/javascript">
 function ajax(){
	//定义ajax请求对象
	var xhr;

	//根据浏览器来创建ajax请求对象
	if (window.ActiveXObject) {
		//IE
		xhr = new ActiveXObject("Microsoft.XMLHTTP");
	} else if (window.XMLHttpRequest) {
		//非IE	
		xhr = new XMLHttpRequest();
	}

	//设置callback回调方法
	xhr.onreadystatechange = callback;

	//获取form表单元素的值
	var username = document.form1.username.value;
	
	//发起请求(页面停留在登录页面)
	xhr.open("post","/MWShop/MWController/selectByName?username=" + username, true);
	//设置头文件
	xhr.setRequestHeader("Content-Type", "application/x-form-urlencoded;charset=UTF-8");
	xhr.send("a=b&=c");

	//callback回调方法
	function callback() {
		var span=document.getElementById("span_name");
		//接收回调信息让页面做出相应的反应
		if (xhr.readyState == 4 && xhr.status == 200) {
			if(xhr.responseText=="success"){
				//用户名已存在,不能注册使用
				span.className="no";
				/* span.innerHTML="用户名已存在,不能注册使用!"; */
				alert("用户名已存在,不能注册使用!");
				document.getElementById("names").value="";
				
			}else if(xhr.responseText=="error"){
				//用户名不存在,能够注册使用
				span.className="ok";
				span.innerHTML="该用户名可以使用";
			}
			}
		}
	} 
 
/*验证密码  */
function dopasss(){
	var password=$("#password");
	var passwords=$("#password2");
	var span=document.getElementById("span");
	if(passwords.val() != password.val()){
		span.innerHTML="两次输入的密码不一致!";
	    span.className="no";
	    return false;
	}else{
		span.innerHTML="";
	}
}

	
/*验证手机号  */
function numberCode(){
	
	var number=document.getElementById("number");
	var span=document.getElementById("span_number");
	
	var name=number.value;
	var reg=/^1[34578]\d{9}$/;
	if(reg.test(name)){
			span.className="ok";
			span.innerHTML="该号码格式正确";
			return true;
		}else{
			span.className="no";
			span.innerHTML="该号码格式错误！请输入十一位纯数字";
			return false;
		}
		}


/*验证邮箱  */
function emailCode(){
	
	var number=document.getElementById("email");
	var span=document.getElementById("span_email");
	
	var name=number.value;
	var reg=/^[a-zA-Z0-9_-]+@[a-zA-Z0-9_-]+(\.[a-zA-Z0-9_-]+)+$/;
	if(reg.test(name)){
			span.className="ok";
			span.innerHTML="该邮箱格式正确";
			return true;
		}else{
			span.className="no";
			span.innerHTML="该号邮箱式错误!";
			return false;
		}
		}
		
/*提交验证 */
function checkonsubmit(){
	var password=$("#password");
	var passwords=$("#password2");
	var span=document.getElementById("span");
	
	
	var number=document.getElementById("number");
	var span=document.getElementById("span_number");
	var name1=number.value;
	var reg1=/^1[34578]\d{9}$/;
	
	var email=document.getElementById("email");
	var span_email=document.getElementById("span_email");
	
	var name= email.value;
	var reg=/^[a-zA-Z0-9_-]+@[a-zA-Z0-9_-]+(\.[a-zA-Z0-9_-]+)+$/;
	
	if(passwords.val() != password.val()){
		alert("两次密码输入不一样!");
	}else if(passwords.val() == password.val()){
		if(reg1.test(name1)){
			if(reg.test(name)){
				return true;
			}else{
				alert("该邮箱格式错误！");
			}
		}else{
			alert("该号码格式错误！请输入十一位纯数字")
		}
	}
}
</script>
